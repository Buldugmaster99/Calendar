<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
	<top>
		<MenuBar BorderPane.alignment="CENTER">
			<Menu mnemonicParsing="false" text="File">
				<CustomMenuItem hideOnClick="false" mnemonicParsing="false" text="ActionR">
					<content>
						<GridPane alignment="CENTER" gridLinesVisible="true" maxWidth="400.0" minWidth="-Infinity" prefWidth="120.0">
							<columnConstraints>
								<ColumnConstraints hgrow="SOMETIMES" />
								<ColumnConstraints hgrow="ALWAYS" maxWidth="200.0" minWidth="0.0" />
								<ColumnConstraints hgrow="SOMETIMES" />
							</columnConstraints>
							<rowConstraints>
								<RowConstraints minHeight="10.0" vgrow="NEVER" />
							</rowConstraints>
							<Label text="Open Proograam" GridPane.hgrow="NEVER" />
							<Label text="Strg + O" GridPane.columnIndex="2" GridPane.hgrow="ALWAYS">
								<font>
									<Font size="9.0" />
								</font>
							</Label>
						</GridPane>
					</content>
				</CustomMenuItem>
				<MenuItem mnemonicParsing="false" text="New" />
				<CustomMenuItem mnemonicParsing="false" text="ActionR">
					<content>
						<GridPane alignment="CENTER" gridLinesVisible="true" maxWidth="400.0" minWidth="-Infinity" prefWidth="120.0">
							<columnConstraints>
								<ColumnConstraints hgrow="SOMETIMES" />
								<ColumnConstraints hgrow="ALWAYS" maxWidth="200.0" minWidth="0.0" />
								<ColumnConstraints hgrow="SOMETIMES" />
							</columnConstraints>
							<rowConstraints>
								<RowConstraints minHeight="10.0" vgrow="NEVER" />
							</rowConstraints>
							<Label text="Save" />
							<Label text="Strg + O" GridPane.columnIndex="2">
								<font>
									<Font size="9.0" />
								</font>
							</Label>
						</GridPane>
					</content>
				</CustomMenuItem>
				<MenuItem mnemonicParsing="false" text="Open…" />
				<Menu mnemonicParsing="false" text="Open Recent" />
				<SeparatorMenuItem mnemonicParsing="false" />
				<MenuItem mnemonicParsing="false" text="Close" />
				<MenuItem mnemonicParsing="false" text="Save" />
				<MenuItem mnemonicParsing="false" text="Save As…" />
				<MenuItem mnemonicParsing="false" text="Revert" />
				<SeparatorMenuItem mnemonicParsing="false" />
				<MenuItem mnemonicParsing="false" text="Preferences…" />
				<SeparatorMenuItem mnemonicParsing="false" />
				<MenuItem mnemonicParsing="false" text="Quit" />
			</Menu>
			<Menu mnemonicParsing="false" text="Help">
				<MenuItem mnemonicParsing="false" text="About MyHelloApp" />
				<MenuItem mnemonicParsing="false" text="Show Shortcuts" />
			</Menu>
		</MenuBar>
	</top>
	<center>
		<TabPane prefHeight="200.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE" BorderPane.alignment="CENTER">
			<Tab closable="false" text="Calender">
				<Pane style="-fx-border-color: 100,100,100;">
					<padding>
						<Insets bottom="20.0" left="20.0" />
					</padding>
					<SplitPane dividerPositions="0.15154440154440155" orientation="VERTICAL" prefHeight="345.0" prefWidth="600.0"
					           style="-fx-border-color: c(&quot;#cecece&quot;);">
						<HBox alignment="CENTER" spacing="40.0">
							<Button mnemonicParsing="false" text="&lt;" />
							<Label text="December">
								<font>
									<Font name="DejaVu Sans Bold" size="26.0" />
								</font>
							</Label>
							<Button mnemonicParsing="false" text="&gt;" />
						</HBox>
						<TableView fixedCellSize="1.0" maxHeight="500.0" maxWidth="600.0" prefHeight="222.0" prefWidth="359.0">
							<columns>
								<TableColumn prefWidth="75.0" text="Monday" />
								<TableColumn prefWidth="75.0" text="Tuesday" />
								<TableColumn prefWidth="75.0" text="Wendsday" />
								<TableColumn prefWidth="75.0" text="Thuesday" />
								<TableColumn prefWidth="75.0" text="Friday" />
								<TableColumn prefWidth="75.0" text="Saturday" />
								<TableColumn prefWidth="75.0" text="Sunday" />
							</columns>
							<columnResizePolicy>
								<TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
							</columnResizePolicy>
						</TableView>
					</SplitPane>
				</Pane>
			</Tab>
		</TabPane>
	</center>
</BorderPane>
